    INTEGER(HID_T)  , INTENT(IN)  :: plid
    CHARACTER(LEN=*), INTENT(IN)  :: name
    TYPE            , INTENT(IN)  :: value
    INTEGER         , INTENT(OUT) :: hdferr
</pre>
<p class="item_name"><strong>Inputs:</strong></p>
