                                         ! identifier
  INTEGER(SIZE_T), INTENT(IN) :: size    ! Vector size 
  INTEGER, INTENT(OUT)       :: hdferr   ! Error code
                                         ! 0 on success and -1 on failure
END SUBROUTINE h5pset_hyper_vector_size_f
	</pre>		
		
